    "                if endp[2] >= mrc1.zdim:\n",
    "                    endp[2] = mrc1.zdim\n",
    "\n",
    "                # setup for numba acc\n",
    "                stp_t = List()\n",
    "                endp_t = List()\n",
    "                pos_t = List()\n",
    "                [stp_t.append(x) for x in stp]\n",
