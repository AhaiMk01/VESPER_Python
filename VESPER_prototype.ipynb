        "def fastVEC(mrc1,mrc2):\r\n",
        "  i,j,k,ind=0,0,0,0\r\n",
        "  cnt=0\r\n",
        "  xydim=mrc1.xdim * mrc1.ydim\r\n",
        "  Ndata = mrc2.xdim * mrc2.ydim * mrc2.xdim\r\n",
        "  print(\"#Start VEC\")\r\n",
        "  dreso = cmd.dreso\r\n",
        "  gstep = mrc1.xwidth\r\n",
        "  fs = (dreso/gstep) * 0.5\r\n",
        "  fsiv = 1.000 / fs\r\n",
