    "@numba.jit(nopython=True)\n",
    "def calc(stp, endp, pos, mrc1_data, fsiv):\n",
    "    dtotal = 0\n",
    "    pos2 = [0.0] * 3\n",
    "\n",
    "    for xp in range(stp[0], endp[0]):\n",
    "        rx = float(xp) - pos[0]\n",
    "        rx = rx ** 2\n",
    "        for yp in range(stp[1], endp[1]):\n",
    "            ry = float(yp) - pos[1]\n",
    "            ry = ry ** 2\n",
    "            for zp in range(stp[2], endp[2]):\n",
    "                rz = float(zp) - pos[2]\n",
    "                rz = rz ** 2\n",
    "                d2 = rx + ry + rz\n",
    "                v = mrc1_data[zp][yp][xp] * math.exp(-1.5 * d2 * fsiv)\n",
    "                dtotal += v\n",
    "                pos2[0] += v * float(xp)\n",
    "                pos2[1] += v * float(yp)\n",
    "                pos2[2] += v * float(zp)\n",
    "    return dtotal, pos2"
   ]
